{
  "presets": [
    "react",
    ["es2015",{"modules":false}],
    "stage-1"
  ],
  "plugins": [
    ["react-hot-loader/babel"],
    [ "transform-decorators-legacy"],
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    [
      "import",
      [
        {
          "libraryName": "antd",
          "style": true
        }
      ]
    ]


  ]
}
