{
  "presets": [
    "react",
    "es2015"
  ],
  "plugins": [
    [
      "import",
      [
        {
          "libraryName": "antd",
          "style": true
        }
      ]
    ],
    [
      "react-transform",
      {
        "transforms": [
          {
            "transform": "react-transform-hmr",
            "imports": [
              "react"
            ],
            "locals": [
              "module"
            ]
          }
        ]
      }
    ]
  ]
}
